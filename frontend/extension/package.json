{
  "name": "nsfw-filter-extension",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:prod": "NODE_ENV=production tsc && vite build",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx --max-warnings 0",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,json,md}\"",
    "svgo": "svgo -f public/svgs -o src/assets/optimized-svgs --config=svgo.config.js",
    "svgr": "svgr src/assets/optimized-svgs --out-dir src/components/icons --config-file svgr.config.cjs",
    "clean": "rimraf dist",
    "prebuild": "npm run clean"
  },
  "dependencies": {
    "@radix-ui/react-select": "2.2.6",
    "@radix-ui/react-slot": "^1.0.4",
    "@radix-ui/react-switch": "^1.1.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "2.1.1",
    "lucide-react": "^0.453.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^6.14.2",
    "recharts": "^3.5.1",
    "tailwind-merge": "^1.14.0",
    "tailwindcss-animate": "1.0.7"
  },
  "devDependencies": {
    "@crxjs/vite-plugin": "^2.2.0",
    "@svgr/cli": "^8.1.0",
    "@types/chrome": "^0.1.12",
    "@types/node": "^24.5.2",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.3",
    "autoprefixer": "^10.4.21",
    "baseline-browser-mapping": "^2.8.31",
    "esbuild": "0.25.10",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^13.20.0",
    "postcss": "^8.5.6",
    "rimraf": "^6.0.1",
    "svgo": "^4.0.0",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.48.0",
    "vite": "^7.1.6"
  }
}
